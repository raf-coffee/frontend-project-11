{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "@babel/eslint-parser"
  },
  "plugins": ["unused-imports", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "error",
    "unused-imports/no-unused-imports": "error"
  }
}
